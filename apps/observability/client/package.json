    "vue": "^3.5.17",
    "vue-chartjs": "^5.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.11.2",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.16",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.0.4",
    "vitest": "^4.0.16",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.2.12"
