    "creative",
    "media",
    "ai-art"
  ],
  "io_schema": {
    "input": {
      "type": "object",
      "required": [
        "prompt"
      ],
      "properties": {
        "size": {
          "enum": [
            "256x256",
            "512x512",
            "1024x1024",
            "1792x1024",
            "1024x1792"
          ],
          "type": "string",
          "default": "1024x1024",
          "description": "Image dimensions"
        },
        "style": {
          "enum": [
            "natural",
            "vivid"
          ],
          "type": "string",
          "default": "natural",
          "description": "Image style"
        },
        "prompt": {
          "type": "string",
          "description": "Text description of the image to generate"
        },
        "quality": {
          "enum": [
            "standard",
            "hd"
          ],
          "type": "string",
          "default": "standard",
          "description": "Image quality level"
        },
        "numberOfImages": {
          "type": "number",
          "default": 1,
          "maximum": 4,
          "minimum": 1,
          "description": "Number of images to generate"
        }
      }
    },
    "output": {
      "type": "object",
      "properties": {
        "images": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "url": {
                "type": "string"
              },
              "assetId": {
                "type": "string"
              },
              "mimeType": {
                "type": "string"
              }
            }
          }
        },
        "success": {
          "type": "boolean"
        },
        "metadata": {
          "type": "object",
          "properties": {
            "model": {
              "type": "string"
            },
            "prompt": {
              "type": "string"
            },
            "provider": {
              "type": "string"
            }
          }
        }
      }
    }
  },
  "capabilities": [
    "image-generation"
