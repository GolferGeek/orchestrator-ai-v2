{
  "slug": "video-generator",
  "organization_slug": [
    "global"
  ],
  "name": "Video Generator",
  "description": "Generates high-quality videos using AI models from OpenAI (Sora 2) and Google (Veo 3). Supports various durations, aspect ratios, and resolutions. Video generation is asynchronous and may take several minutes.",
  "version": "1.0.0",
  "agent_type": "media",
  "department": "creative",
  "tags": [
    "video",
    "generation",
    "creative",
    "media",
    "ai-video",
    "sora",
    "veo"
  ],
  "io_schema": {
    "input": {
      "type": "object",
      "required": [
        "prompt"
      ],
      "properties": {
        "fps": {
          "enum": [
            24,
            30,
            60
          ],
          "type": "number",
          "default": 24,
          "description": "Frames per second"
        },
        "prompt": {
          "type": "string",
          "description": "Text description of the video to generate"
        },
        "duration": {
          "type": "number",
          "default": 10,
          "maximum": 60,
          "minimum": 5,
          "description": "Video duration in seconds (5-60)"
        },
        "resolution": {
          "enum": [
            "480p",
            "720p",
            "1080p"
          ],
          "type": "string",
          "default": "720p",
          "description": "Video resolution"
        },
        "aspectRatio": {
          "enum": [
            "16:9",
            "9:16",
            "1:1"
          ],
          "type": "string",
          "default": "16:9",
          "description": "Video aspect ratio"
        },
        "firstFrameImageUrl": {
          "type": "string",
          "description": "Optional URL of an image to use as the first frame (image-to-video)"
        }
      }
    },
    "output": {
      "type": "object",
      "properties": {
        "videos": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "url": {
                "type": "string"
              },
              "assetId": {
                "type": "string"
              },
              "mimeType": {
                "type": "string"
              },
              "durationSeconds": {
                "type": "number"
              }
            }
          }
        },
        "success": {
          "type": "boolean"
        },
        "metadata": {
          "type": "object",
          "properties": {
            "model": {
              "type": "string"
            },
            "prompt": {
              "type": "string"
            },
            "provider": {
              "type": "string"
            },
            "processingTimeSeconds": {
              "type": "number"
            }
          }
        }
      }
    }
  },
  "capabilities": [
    "video-generation"
  ],
  "context": "{\"input_modes\": [\"text/plain\", \"image/png\", \"image/jpeg\"], \"output_modes\": [\"video/mp4\", \"video/webm\"]}",
  "endpoint": null,
  "llm_config": null,
  "metadata": {
    "mediaType": "video",
    "defaultModel": "sora-2",
    "asyncGeneration": true,
    "defaultProvider": "openai",
    "supportedModels": {
      "google": [
        "veo-3-generate"
      ],
      "openai": [
        "sora-2"
      ]
    },
    "supportedProviders": [
      "openai",
      "google"
    ],
    "executionCapabilities": {
      "canPlan": false,
      "canBuild": true,
      "canConverse": false
    },
    "estimatedProcessingTime": "2-10 minutes depending on duration and resolution"
  },
  "created_at": "2025-12-24T15:21:49.854631+00:00",
  "updated_at": "2025-12-24T15:21:49.854631+00:00"
}